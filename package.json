{
  "name": "@enkidevs/curriculum-parser",
  "version": "0.1.0",
  "description": "Enki curriculum parser",
  "main": "dist/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir dist",
    "build:es": "BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:es && npm run build:cjs",
    "clean": "rimraf dist",
    "format": "prettier-standard 'src/**/*.js'",
    "lint": "eslint . --ignore-path .gitignore",
    "prebuild": "npm run clean",
    "prepublish": "npm run test && npm run clean && npm run build",
    "pretest": "npm run lint",
    "test:watch": "jest --watchAll",
    "test": "npm run lint && jest --notify"
  },
  "keywords": [
    "enki",
    "curriculum",
    "parser",
    "ast"
  ],
  "pre-commit": [
    "format",
    "test"
  ],
  "files": [
    "es",
    "dist"
  ],
  "author": "Nemanja Stojanovic <me@nem035.com>",
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enkidevs/curriculum-parser.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enkidevs/curriculum-parser/issues"
  },
  "homepage": "https://github.com/enkidevs/curriculum-parser#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint-plugin-import": "^2.9.0",
    "jest": "^22.4.2",
    "jest-in-case": "^1.0.2",
    "lint-staged": "^7.0.0",
    "lodash.mergewith": "^4.6.1",
    "lodash.omitby": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "pre-commit": "^1.2.2",
    "prettier-standard": "^8.0.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "js-yaml": "^3.10.0",
    "remark-breaks": "^1.0.0",
    "remark-parse": "^5.0.0",
    "unified": "^6.1.6",
    "unist-util-visit": "^1.3.0"
  },
  "jest": {
    "testRegex": "/src.*__tests__/.*\\.test.js$"
  }
}
